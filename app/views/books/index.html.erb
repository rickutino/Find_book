<h1>Book#index</h1>
<table>
  <% @books.each do |book| %>
  <tr>
    <td><%= book.title %></td>
    <td><%= link_to 'Edit', edit_book_path(book), method: 'get'%></td>
    <td><%= link_to 'Remove', book_path(book), method: :delete, data: { confirm: '削除してもよろしいですか?' } %></td>
  <% end %>
</table>
<%= link_to "ホームページへ", new_search_path %>
